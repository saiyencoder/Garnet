
<div class="panel panel-default">
  <div class="panel-heading">Seasons</div>
</div>

<% if current_player && current_player.commissioner %>
  <a href="/seasons/new">Start a new Season</a>
<% end %>
<div class="panel panel-default">
  <div class="panel-body">
    <table class="table table-bordered">
      <% @seasons.each do |season| %>
      <tr>
        <th class="col-md-1">Season</th>
        <th class="col-md-1">Year</th>
      </tr>
      <tr>
        <th><a href="/seasons/<%= season.id %>"><%= season.quarter %></th>
        <th><a href="/seasons/<%= season.id %>"><%= season.year %></th>
      </tr>
      <% end %>
    </table>
</div>








